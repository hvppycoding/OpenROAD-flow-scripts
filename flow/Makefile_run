FOLDERS=(_aes _ethmac _ibex _jpeg _riscv32i)
FILES=(config_rest.mk)

for FOLDER in "${FOLDERS[@]}"; do
	for FILE in "${FILES[@]}"; do
		echo "make clean clean_all MYFOLDER=$FOLDER MYFILE=$FILE"
		make clean clean_all MYFOLDER=$FOLDER MYFILE=$FILE
		echo "make MYFOLDER=$FOLDER MYFILE=$FILE"
		make MYFOLDER=$FOLDER MYFILE=$FILE
	done
done

